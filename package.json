{
  "name": "vue3template",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://github.com/HeroMeiKong/MyVue3Template",
  "bugs": {
    "url": "https://github.com/HeroMeiKong/MyVue3Template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HeroMeiKong/MyVue3Template.git"
  },
  "license": "MIT",
  "author": {
    "name": "V3T",
    "email": "xiaopangfeng4@hotmail.com",
    "url": "https://github.com/HeroMeiKong"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:dev": "vue-tsc && vite build --mode development",
    "build:pre": "vue-tsc && vite build --mode staging",
    "preview": "vite preview"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "prettier --write",
      "eslint --fix",
      "stylelint --fix"
    ],
    "*.{scss,less,styl,html}": [
      "prettier --write",
      "stylelint --fix"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@unocss/reset": "^0.53.5",
    "@vueuse/core": "^10.2.1",
    "ant-design-vue": "4.0.0",
    "axios": "^1.4.0",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.9",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.4",
    "pinia-plugin-persistedstate": "^3.1.0",
    "qs": "^6.11.2",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.6.7",
    "@iconify-json/mdi": "^1.1.53",
    "@types/crypto-js": "^4.1.1",
    "@types/lodash-es": "^4.17.8",
    "@types/mockjs": "^1.0.7",
    "@types/node": "^20.4.2",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@unocss/preset-rem-to-px": "^0.53.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "cz-git": "^1.7.0",
    "dotenv": "^16.3.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "rollup": "^3.26.3",
    "sass": "^1.64.0",
    "typescript": "^5.1.6",
    "unocss": "^0.53.5",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-icons": "^0.16.5",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.5",
    "vite-plugin-mock": "^2.9.8",
    "vue-tsc": "^1.8.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
